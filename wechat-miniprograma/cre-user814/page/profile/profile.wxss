@import "../../common/weui.wxss";
page,
.page,
.page__bd{
height: 100%;
}
.page__bd{
padding-bottom: 0;
}
.weui-tab__content{
padding-top: 20px;
text-align: center;
}
.weui-navbar__item{ background: #FFF; position:relative;display:block;-webkit-box-flex:1;-webkit-flex:1;flex:1;padding:13px 0;text-align:center;font-size:0}
.weui-navbar__item.weui-bar__item_on{color:#FFF; background-color: #1aad19;}

.weui-navbar__title {
display:inline-block;
font-size:15px;
max-width:8em;
width:auto;
overflow:hidden;
text-overflow:ellipsis;
white-space:nowrap;
word-wrap:normal;
}
.apply-choice{
width: 40rpx;
height: 40rpx;
margin-right: 12rpx;
vertical-align: text-bottom;
}
view{
font-size: 30rpx;
font-weight: 300;
}
.body{
padding: 25rpx 25rpx 0;
background-color: #fff;
}
.radio-group{
position: relative;
display: flex;
width: 100%;
height: 70rpx;
color:#695f9b;
}
.radio{
display: inline-block;
flex: 1;
height: 100%;
border-color: #695f9b;
border-width: 1rpx 1rpx 1rpx 0;
border-style: solid;
box-sizing: border-box;
color: #695f9b;
text-align: center;
line-height: 70rpx;
}
.radio:first-child{
border-left: 1px solid #695f9b;
border-top-left-radius: 8rpx;
border-bottom-left-radius: 8rpx;
}
.radio:last-child{
border-top-right-radius: 8rpx;
border-bottom-right-radius: 8rpx;
}
.radio-item{
position: absolute;
left: -99999rpx;
top: -99999rpx;
height: 0;
width: 0;
visibility: hidden;
}
.type-group{
position: relative;
width: 100%;
text-align: justify;
color:#695f9b;
overflow: hidden;
}
.type-item{
position: absolute;
left: -99999rpx;
top: -99999rpx;
height: 0;
width: 0;
visibility: hidden;
}
.type-label{
display: inline-block;
margin:0 25rpx 25rpx;
width: 137.5rpx;
height: 65rpx;
line-height: 65rpx;
text-align: center;
border-radius: 8rpx;
border: 1px solid #695f9b;
}
.active{
background-color: #695f9b;
color: #fff;
transition: all .3s;
}
.mar-top-60{
margin-top: 60rpx;
}
page{
background-color: #f3f2f7;
}
.disabled{
border-color: #aaa;
background-color: #f0f0f0;
color: #aaa;
}
.btn-submit{
height: 70rpx;
line-height: 70rpx;
text-align: center;
border: 1px solid #1aad19;
color:#1aad19;
background-color: #FFF;
width: 35%;
float: right;
border-radius: 50rpx;
font-size: 28rpx;
margin-top: 10px;
}
.btn-submit-a{
height: 70rpx;
line-height: 70rpx;
text-align: center;
margin-right: 10px;
border: 1px solid #ff7800;
color:#ff7800;
background-color: #FFF;
width: 25%;
float: right;
border-radius: 50rpx;
font-size: 28rpx;
margin-top: 10px;
}
.btn-submit-b{
height: 70rpx;
line-height: 70rpx;
text-align: center;
margin-right: 10px;
border: 1px solid #999;
color:#999;
background-color: #FFF;
width: 33%;
float: right;
border-radius: 50rpx;
font-size: 28rpx;
margin-top: 10px;
}
.btn-submit-c{
height: 70rpx;
line-height: 70rpx;
text-align: center;
margin-right: 10px;
border: 1px solid #999;
color:#999;
background-color: #FFF;
width: 25%;
float: right;
border-radius: 50rpx;
font-size: 28rpx;
margin-top: 10px;
}
navigator{
color: #695f9b;
display: inline-block;
}